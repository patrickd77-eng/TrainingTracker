@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">You do not have access to this resource, or you're making a bad request, or the item does not exist.</p>
    <p class="text-danger">Please contact the site owner.</p>
</header>
